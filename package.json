{
  "name": "ureed-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:local": "ng serve -c local",
    "start:dev": "ng serve -c dev",
    "start:staging": "export NODE_ENV=production && ng serve -c staging",
    "start:prod": "export NODE_ENV=production && ng serve --prod",
    "build": "node node_modules/@angular/cli/bin/ng build --aot",
    "build:dev": "node node_modules/@angular/cli/bin/ng build -c dev --aot",
    "build:staging": "export NODE_ENV=production && node node_modules/@angular/cli/bin/ng build -c staging --aot",
    "build:prod": "export NODE_ENV=production && node node_modules/@angular/cli/bin/ng build --prod --aot",
    "lint": "./node_modules/.bin/eslint \"./src/**/*.ts\"",
    "eslint": "./node_modules/.bin/eslint \"./src/**/*.ts\"",
    "eslint-fix": "./node_modules/.bin/eslint \"./src/**/*.ts\" --fix",
    "test": "ng test --watch=false --progress=false --browsers=ChromeHeadlessNoSandbox",
    "test-watch": "ng test --watch=true --progress=true --browsers=Chrome",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@apollo/client": "^3.0.0",
    "@garygrossgarten/ngx-tailwind": "^0.4.0",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "apollo-angular": "^2.0.4",
    "apollo-link-error": "^1.1.13",
    "flatted": "^3.1.0",
    "graphql": "^15.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@ngrx/schematics": "^10.0.1",
    "@schematics/angular": "~10.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-node": "11",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "4",
    "eslint-plugin-rxjs": "^1.0.0",
    "eslint-plugin-standard": "4",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "ngx-build-plus": "latest",
    "postcss": "^8.0.0",
    "postcss-import": "latest",
    "postcss-loader": "latest",
    "postcss-scss": "latest",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "tailwindcss": "latest",
    "tailwindcss-dir": "^4.0.0",
    "ts-node": "~8.3.0",
